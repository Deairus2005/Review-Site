@model IEnumerable<ReviewsSite.Models.Review><h1>Reviews</h1><hr /><div>    <a asp-controller="Home" asp-action="Index">Return to Home</a></div><table class="reviewTable">    <tr>        <th>Product Name</th>        <th>Product Review</th>        <th>Product Rating</th>    </tr>    @foreach (var review in Model)    {<tr class="innerTr">    <td>@review.Product.Name</td>    <td>@review.Rating out of 5 stars</td>    <td>@review.ReviewContent</td></tr>}</table>